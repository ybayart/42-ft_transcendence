<p id="notice"><%= notice %></p>

<h1>Admin Rooms</h1>

<table class="table">
	<thead>
		<tr>
			<th scope="col">Name</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @rooms.each do |room| %>
			<tr>
				<td><%= room.name %></td>
				<td><%= link_to 'Show', admin_room_path(room) %></td>
				<td><%= link_to 'Destroy', room_path(room, back: true), method: :delete, data: { confirm: 'Are you sure?' }, remote: true %></td>
			</tr>
		<% end %>
	</tbody>
</table>
