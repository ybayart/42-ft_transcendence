<p id="notice"><%= notice %></p>

<h1>Admin Guilds</h1>

<table class="table">
	<thead>
		<tr>
			<th scope="col">Name</th>
			<th scope="col">Anagram</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @guilds.each do |guild| %>
			<tr>
				<td><%= guild.name %></td>
				<td><%= guild.anagram %></td>
				<td><%= link_to 'Show', admin_guild_path(guild) %></td>
			</tr>
		<% end %>
	</tbody>
</table>
