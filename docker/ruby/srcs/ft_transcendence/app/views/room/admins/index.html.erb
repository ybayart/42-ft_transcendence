<p id="notice"><%= notice %></p>

<h1>Room Admins</h1>

<table>
	<thead>
		<tr>
			<th>User</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @room_admins.each do |room_admin| %>
			<tr>
				<td><%= create_user_link(room_admin) %></td>
				<td><%= link_to 'Destroy', room_admin_path(@room, room_admin), method: :delete, data: { confirm: 'Are you sure?' }, remote: true %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<br>

<%= link_to 'New Room Admin', new_room_admin_path %> |
<%= link_to 'Back', @room %>
